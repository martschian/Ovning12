@model VehicleTypes

@{
    string vehicleType;
    switch(Model) {
        case VehicleTypes.Boat:
            vehicleType = "Båt";
            break;
        case VehicleTypes.Car:
            vehicleType = "Bil";
            break;
        case VehicleTypes.Motorcyle:
            vehicleType = "Motorcykel";
            break;
        case VehicleTypes.Bus:
            vehicleType = "Buss";
            break;

        default:
            vehicleType = Model.ToString();
            break;
    }
}

@vehicleType